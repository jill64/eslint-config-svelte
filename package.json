  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "tsc && npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "6.11.3",
