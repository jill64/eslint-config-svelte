{
  "name": "@jill64/eslint-config-svelte",
  "description": "ðŸ”¹Pre-Defined ESLint Flat Config for Svelte",
  "version": "0.0.3",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/eslint-config-svelte/issues",
  "homepage": "https://github.com/jill64/eslint-config-svelte#readme",
  "files": [
    "dist",
    "!**/*.test.*",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/eslint-config-svelte.git"
